@article{2003m,
  title = {Eucommia ({{Eucommiaceae}}), a Potential Biothermometer for the Reconstruction of Paleoenvironments.},
  author = {Wang, Yu-Fei and Li, Cheng-Sen and Collinson, Margaret E. and Lin, Jian and Sun, Qi-Gao},
  year = {2003},
  month = jan,
  journal = {American journal of botany},
  volume = {90},
  number = {1},
  pages = {1--7},
  address = {{United States}},
  issn = {0002-9122},
  doi = {10.3732/ajb.90.1.1},
  abstract = {The living trees of Eucommia ulmoides, an endemic species in China, grow from 200 to 1700 m above sea level, within the geographic range from 102\textdegree{} E to 118\textdegree{} E and  from 25\textdegree{} N to 35\textdegree{} N. Spring temperatures in these regions vary from 12.3\textdegree C to  20.1\textdegree C. A physiological study (using germination tests) of E. ulmoides has been  undertaken to test the role of spring temperature as a factor controlling the  distribution of Eucommia. Results show that the spring temperature is a limiting  factor for Eucommia seed germination and hence for the distribution pattern of  the genus. The suitable range of temperature for seed germination, established  experimentally, is from 13\textdegree C to 22\textdegree C, with an optimum of 18\textdegree C. Specimens of  fossil Eucommia cf. ulmoides, preserved as a branch segment and leaves, showing  the distinctive latex, were found in Middle Miocene sediments of Shanwang  Formation, Shandong Province, East China. If the climatic tolerances documented  here for E. ulmoides are extrapolated to Shanwang, they are in fact consistent  with other predictions of the paleoclimate at this site, indicative of the  potential value of Eucommia as a biothermometer. These Miocene fossils, and one  previously described Eocene fruit specimen, prove the former existence of  Eucommia in China in addition to North America and Europe. This confirms that the  genus is not a recent arrival in China and extends our understanding of the past  biogeography of the genus.},
  langid = {english},
  pmid = {21659074},
  file = {/mnt/data/wizard/Zotero/storage/LEJ447QI/Wang et al. - 2003 - Eucommia (Eucommiaceae), a potential biothermomete.pdf}
}

