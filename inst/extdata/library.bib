  shorttitle = {\lbrace{}\lbrace{}AutoDock Vina\rbrace{}\rbrace{} 1.2.0},
  title = {AutoDock Vina 1.2.0: New Docking Methods, Expanded Force Field, and Python Bindings},
  volume = {61},
