@article{bian_robust_2020,
  title = {Robust, Reproducible and Quantitative Analysis of Thousands of Proteomes by Micro-Flow {{LC}}\textendash{{MS}}/{{MS}}},
  author = {Bian, Yangyang and Zheng, Runsheng and Bayer, Florian P. and Wong, Cassandra and Chang, Yun-Chien and Meng, Chen and Zolg, Daniel P. and Reinecke, Maria and Zecha, Jana and Wiechmann, Svenja and Heinzlmeir, Stephanie and Scherr, Johannes and Hemmer, Bernhard and Baynham, Mike and Gingras, Anne-Claude and Boychenko, Oleksandr and Kuster, Bernhard},
  year = {2020},
