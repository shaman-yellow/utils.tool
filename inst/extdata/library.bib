  file = {/mnt/data/wizard/Zotero/storage/U8PDI7EK/Altay et al. - 2021 - Combined Metabolic Activators Accelerates Recovery.pdf},
  issn = {2198-3844},
  journal = {Advanced Science (Weinheim, Baden-Wurttemberg, Germany)},
  keywords = {Adult,Aged,Antioxidants,combined metabolic activators,COVID-19,Double-Blind Method,Female,Humans,Inflammation,Male,Metabolome,metabolomics,Middle Aged,omics data,Proteins,Proteome,proteomics,Young Adult},
  langid = {english},
