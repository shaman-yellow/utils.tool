}

@article{2021dd,
  title = {Multi-Platform Omics Analysis Reveals Molecular Signature for {{COVID-19}} Pathogenesis, Prognosis and Drug Target Discovery},
  author = {Li, Yuming and Hou, Guixue and Zhou, Haibo and Wang, Yanqun and Tun, Hein Min and Zhu, Airu and Zhao, Jingxian and Xiao, Fei and Lin, Shanwen and Liu, Dongdong and Zhou, Dunrong and Mai, Lang and Zhang, Lu and Zhang, Zhaoyong and Kuang, Lijun and Guan, Jiao and Chen, Qiushi and Wen, Liyan and Zhang, Yanjun and Zhuo, Jianfen and Li, Fang and Zhuang, Zhen and Chen, Zhao and Luo, Ling and Liu, Donglan and Chen, Chunke and Gan, Mian and Zhong, Nanshan and Zhao, Jincun and Ren, Yan and Xu, Yonghao},
