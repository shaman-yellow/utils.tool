@article{2018as,
  title = {Quality Evaluation of {{Eucommiae Cortex}} Processed by Different Methods and "Sweating" Conditions Based on Simultaneous Determination of Multiple Bioactive  Constituents Combined with Gray Relational Analysis.},
  author = {Yan, Ying and Cai, Hao and Zhao, Hui and Chen, Cuihua and Zou, Lisi and Liu, Xunhong and Chai, Chuan and Wang, Shengnan and Hua, Yujiao},
