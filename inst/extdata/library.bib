  keywords = {Animals,Chemistry; Pharmaceutical,Drugs; Chinese Herbal/adverse effects/chemistry/*pharmacology/therapeutic use,Eucommiaceae/*chemistry,Humans}
}

@article{2013r,
  title = {Preparative Isolation and Purification of Seven Main Antioxidants from {{Eucommia}} Ulmoides {{Oliv}}. ({{Du-zhong}}) Leaves Using {{HSCCC}} Guided by {{DPPH-HPLC}} Experiment.},
  author = {Dai, Xingping and Huang, Qiong and Zhou, Boting and Gong, Zhicheng and Liu, Zhaoqian and Shi, Shuyun},
  year = {2013},
  month = aug,
  journal = {Food chemistry},
  volume = {139},
  number = {1-4},
  pages = {563--570},
  address = {{England}},
  issn = {1873-7072 0308-8146},
  doi = {10.1016/j.foodchem.2013.02.006},
  abstract = {Seven antioxidants were purified from Eucommia ulmoides Oliv. leaves using HSCCC guided by DPPH-HPLC experiment. HSCCC was successfully used to separate target  antioxidants by three runs with different solvent systems after D101 column  chromatography fractionation. Ethyl acetate-n-butanol-water (1:2:3, v/v/v) was  selected as the optimum solvent system to purify geniposidic acid. Ethyl  acetate-ethanol-water (4:1:5, v/v/v) was used to isolate caffeic acid,  chlorogenic acid and ferulic acid. While three flavonoids,  quercetin-3-O-sambubioside, rutin and isoquercitrin were purified by petroleum  ether-ethyl acetate-methanol-water (1:5:1:5, v/v/v/v). The structures were  identified by MS and NMR. Antioxidant activities were assessed, and compounds 2-7  showed strong antioxidant activities. This is the first report about separation  of antioxidants from E. ulmoides leaves by HSCCC. The results indicated that the  combinative methods using DPPH-HPLC and HSCCC could be widely applied for  screening and isolation of antioxidants from complex extracts.},
  copyright = {Copyright \textcopyright{} 2013 Elsevier Ltd. All rights reserved.},
