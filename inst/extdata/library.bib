  author = {Gu, Juan and Wang, Jun-Jie and Yan, Jin and Cui, Chang-Fu and Wu, Wei-Hua and Li, Ling and Wang, Zhen-Shan and Yu, Min and Gao, Na and Liu, Li and Ouyang, Dong-Sheng},
  year = {2011},
  month = jan,
  journal = {Journal of ethnopharmacology},
  volume = {133},
  number = {1},
  pages = {6--13},
  address = {{Ireland}},
  issn = {1872-7573 0378-8741},
  doi = {10.1016/j.jep.2010.08.055},
  abstract = {AIM OF THE STUDY: To investigate the effects of lignans extracted from Eucommia ulmoides and epalrestat on vascular remodeling in spontaneously hypertensive  rats. MATERIALS AND METHODS: Ten-week-old male spontaneously hypertensive rats  were randomly divided into 3 groups (12 rats each group), and treated orally with  100 mg/kg/d of captopril (an angiotensin-converting enzyme inhibitor), 100  mg/kg/d of epalrestat (an aldose reductase inhibitor) and 300 mg/kg/d of lignans  by gavage daily for 16 weeks, respectively. Sex-, age-, and number-matched  spontaneously hypertensive rats and normotensive Wistar Kyoto rats, were treated  with distilled water (vehicle) as controls. The rats were weighed weekly. Mean  arterial blood pressure and heart rate were measured periodically by non-invasive  blood pressure monitoring. They were sacrificed at the end of experiment  (26-week-old). Superior mesenteric artery and aorta were isolated for  determination of histomorphometry and the expression of aldose reductase by  immunohistochemistry. RESULTS: Captopril and lignans, but not epalrestat,  decreased mean arterial blood pressure in spontaneously hypertensive rats.  Vascular remodeling was improved in all three treated groups by histomorphometry.  CONCLUSIONS: Both lignans and epalrestat reversed hypertensive vascular  remodeling. Aldose reductase played a vital role in the pathologic process of  hypertensive vascular remodeling rather than elevation of blood pressure. These  data suggested that aldose reductase could be a new therapeutic target for the  treatment of cardiovascular diseases.},
  copyright = {Copyright \textcopyright{} 2010 Elsevier Ireland Ltd. All rights reserved.},
  langid = {english},
  pmid = {20817083},
  keywords = {*Eucommiaceae,Aldehyde Reductase/*antagonists \& inhibitors/metabolism,Animals,Aorta/enzymology/pathology,Blood Pressure/drug effects,Captopril/pharmacology,Drugs; Chinese Herbal,Enzyme Inhibitors/*pharmacology,Female,Heart Rate/drug effects,Hypertension/*drug therapy/enzymology/*pathology,Lignans/*pharmacology,Male,Mesenteric Artery; Superior/enzymology/pathology,Plant Bark,Random Allocation,Rats,Rats; Inbred SHR,Rats; Wistar,Rhodanine/*analogs \& derivatives/pharmacology,Thiazolidines/*pharmacology},
  file = {/mnt/data/wizard/Zotero/storage/3CYS7X32/Gu et al. - 2011 - Effects of lignans extracted from Eucommia ulmoide.pdf}
}

