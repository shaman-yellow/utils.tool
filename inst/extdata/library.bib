  author = {Li, Lili and Guo, Yupin and Zhao, Lianfei and Zu, Yuangang and Gu, Huiyan and Yang, Lei},
  year = {2015},
  month = oct,
  journal = {Molecules (Basel, Switzerland)},
  volume = {20},
