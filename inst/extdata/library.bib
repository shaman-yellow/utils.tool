@article{2018at,
  title = {Eucommia Ulmoides Leaf ({{EUL}}) Extract Enhances {{NO}} Production in Ox-{{LDL-treated}} Human Endothelial Cells.},
  author = {Lee, Geum-Hwa and Lee, Hwa-Young and Choi, Min-Kyung and Choi, An-Hong and Shin, Tai-Sun and Chae, Han-Jung},
