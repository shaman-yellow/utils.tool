  title = {{[Chemical constituents of Eucommia ulmoides].}},
  author = {Wang, Shuang-Yan and Ding, Lin-Fen and Wu, Xing-De and Wang, Hai-Yin and Zhao, Qin-Shi and Song, Liu-Dong},
