  shorttitle = {Traditionally {{Used Plants}} in the {{Treatment}} of {{Diabetes Mellitus}}},
  title = {Traditionally Used Plants in the Treatment of Diabetes Mellitus: Screening for Uptake Inhibition of Glucose and Fructose in the Caco2-Cell Model},
