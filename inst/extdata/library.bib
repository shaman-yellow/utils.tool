}

@article{2012p,
  title = {The {{Ca2}}+ -Dependent {{DNases}} Are Involved in Secondary Xylem Development in {{Eucommia}} Ulmoides.},
  author = {Chen, Hui-Min and Pang, Yu and Zeng, Jun and Ding, Qi and Yin, Shen-Yi and Liu, Chao and Lu, Meng-Zhu and Cui, Ke-Ming and He, Xin-Qiang},
  year = {2012},
  month = jul,
  journal = {Journal of integrative plant biology},
  volume = {54},
  number = {7},
  pages = {456--470},
  address = {{China (Republic : 1949- )}},
  issn = {1744-7909 1672-9072},
  doi = {10.1111/j.1744-7909.2012.01134.x},
  abstract = {Secondary xylem development has long been recognized as a typical case of programmed cell death (PCD) in plants. During PCD, the degradation of genomic DNA  is catalyzed by endonucleases. However, to date, no endonuclease has been shown  to participate in secondary xylem development. Two novel Ca(2+) -dependent DNase  genes, EuCaN1 and EuCaN2, were identified from the differentiating secondary  xylem of the tree Eucommia ulmoides Oliv., their functions were studied by DNase  activity assay, in situ hybridization, protein immunolocalization and  virus-induced gene silencing experiments. Full-length cDNAs of EuCaN1 and EuCaN2  contained an open reading frame of 987 bp, encoding two proteins of 328 amino  acids with SNase-like functional domains. The genomic DNA sequence for EuCaN1 had  no introns, while EuCaN2 had 8 introns. EuCaN1 and EuCaN2 digested ssDNA and  dsDNA with Ca(2+) -dependence at neutral pH. Their expression was confined to  differentiating secondary xylem cells and the proteins were localized in the  nucleus. Their activity dynamics was closely correlated with secondary xylem  development. Secondary xylem cell differentiation is influenced by RNAi of  endonuclease genes. The results provide evidence that the Ca(2+) -dependent  DNases are involved in secondary xylem development.},
  copyright = {\textcopyright{} 2012 Institute of Botany, Chinese Academy of Sciences.},
  langid = {english},
  pmid = {22694768},
  keywords = {Deoxyribonucleases/genetics/*metabolism,Eucommiaceae/*enzymology/*growth \& development,Gene Expression Regulation; Plant/physiology,Plant Proteins/genetics/*metabolism,Xylem/*enzymology/*growth \& development},
  file = {/mnt/data/wizard/Zotero/storage/TRX8M9KT/Chen et al. - 2012 - The Ca2+ -dependent DNases are involved in seconda.pdf}
}

@article{2012q,
  title = {Classical to Current Approach for Treatment of Psoriasis: A Review.},
  author = {Rahman, Mahfoozur and Alam, Kainat and Ahmad, Mohammad Zaki and Gupta, Gaurav and Afzal, Muhammad and Akhter, Sohail and Kazmi, Imran and Jyoti and Ahmad, Farhan Jalees and Anwar, Firoz},
  year = {2012},
  month = sep,
  journal = {Endocrine, metabolic \& immune disorders drug targets},
  volume = {12},
