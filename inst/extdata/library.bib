  keywords = {adsorption,Adsorption,Carbon,Carbon/*chemistry,cytotoxicity.,Delayed-Action Preparations/*chemistry,Drug Carriers/chemistry,Eucommia ulmoides Oliv. leaf,Eucommiaceae/*chemistry,Plant Extracts/*chemistry,Plant Leaves/*chemistry,polylactic acid,release}
