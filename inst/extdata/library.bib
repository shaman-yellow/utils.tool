  keywords = {Adult,Aged,\lbrace{}Aged, 80 and over\rbrace{},Ascites,Blood Proteins,CHILD-PUGH score,Female,Galectin 3,Galectin-3,Galectins,Hepatic Veins,Humans,Liver,\lbrace{}Liver Cirrhosis, Alcoholic\rbrace{},Lysophosphatidylcholines,Male,Middle Aged,Phosphatidylcholine,Prothrombin Time,Severity of Illness Index},
