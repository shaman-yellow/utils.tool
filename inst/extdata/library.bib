@article{2021ck,
  title = {Pinoresinol Diglucoside Attenuates Neuroinflammation, Apoptosis and Oxidative Stress in a Mice Model with {{Alzheimer}}'s Disease.},
  author = {Lei, Shuangyuan and Wu, Suli and Wang, Guanzhuo and Li, Bing and Liu, Bin and Lei, Xia},
  year = {2021},
  month = feb,
  journal = {Neuroreport},
  volume = {32},
  number = {3},
  pages = {259--267},
  address = {{England}},
  issn = {1473-558X 0959-4965},
  doi = {10.1097/WNR.0000000000001583},
  abstract = {For Alzheimer's disease (AD), there is still no effective treatment strategy. Pinoresinol diglucoside (PDG) is one of the major lignans isolated from Eucommia  ulmoides. It is endowed with multiple pharmacological activities, including  anti-inflammatory, antioxidant and anticancer activities. In this study, we  investigated the potential neuroprotective functions of PDG in AD. Mice model  with AD was established adopting stereotactic hippocampal injection of A{$\beta$}1-42  (410\,pmol/mouse), and 3\,days later, mice were administrated with 5 and 10\,mg/kg  PDG by intragastric administration every day for 3\,weeks. Morris water maze and  Y-maze tests demonstrated that PDG treatment could markedly reverse  A{$\beta$}1-42-induced memory impairment in mice. It is found that PDG restrained the  release of proinflammatory cytokines (tumor necrosis factor {$\alpha$} and interleukin  1{$\beta$}), reactive oxygen species and malondialdehyde, and promoted the activity of  the antioxidant enzyme (superoxide dismutase and catalase) by quantitative  real-time-PCR, colorimetric method and ELISA assay. Western blot assay results  have shown that PDG could also upregulate the ratio of Bcl-2/Bax and downregulate  cytochrome c and cleaved caspase-3 expressions, thereby inhibiting neuronal  apoptosis. Furthermore, PDG also significantly reduced the expression of  Toll-like receptor 4 (TLR4) and the activation of nuclear factor-{$\kappa$}B (NF-{$\kappa$}B) p65,  and promoted nuclear factor E2-related factor 2 (Nrf2) and heme oxygenase 1  (HO-1) expressions. In conclusion, PDG can attenuate neuroinflammation, neuronal  apoptosis and oxidative stress through the TLR4/NF-{$\kappa$}B and Nrf2/HO-1 pathways, and  ameliorate memory dysfunction induced by A{$\beta$}1-42 in mice.},
  copyright = {Copyright \textcopyright{} 2021 Wolters Kluwer Health, Inc. All rights reserved.},
  langid = {english},
  pmid = {33470758},
  keywords = {Alzheimer Disease/*metabolism/physiopathology,Amyloid beta-Peptides/toxicity,Animals,Apoptosis/*drug effects,Catalase/drug effects/genetics,Disease Models; Animal,Hippocampus,Inflammation/*metabolism,Injections,Interleukin-1beta/drug effects/metabolism,Lignans/*pharmacology,Malondialdehyde/metabolism,Mice,Morris Water Maze Test,Oxidative Stress/*drug effects,Peptide Fragments/toxicity,Reactive Oxygen Species/metabolism,Stereotaxic Techniques,Superoxide Dismutase/drug effects/genetics,Toll-Like Receptor 4/drug effects/metabolism,Transcription Factor RelA/drug effects/metabolism,Tumor Necrosis Factor-alpha/drug effects/metabolism}
}

