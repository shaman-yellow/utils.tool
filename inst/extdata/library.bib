  journal = {Arzneimittel-Forschung},
  keywords = {Aggression,Amphetamine,Animals,Apomorphine,Avoidance Learning,\lbrace{}Behavior, Animal\rbrace{},Blood Pressure,Chlordiazepoxide,Chlorpromazine,Dibenzoxepins,\lbrace{}Drug Evaluation, Preclinical\rbrace{},Epinephrine,Guinea Pigs,Histamine H1 Antagonists,Humans,Lethal Dose 50,Male,Mice,Motor Activity,Muscle Contraction,Piperazines,Rats,Sleep,Time Factors,Tranquilizing Agents},
  langid = {english},
