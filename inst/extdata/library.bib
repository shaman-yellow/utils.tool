  file = {/mnt/data/wizard/Zotero/storage/PH9XEA7P/Talmon et al. - 2023 - Transcriptomic profile comparison of monocytes fro.pdf},
  issn = {1932-6203},
  journal = {PloS One},
  keywords = {Abatacept,\lbrace{}Arthritis, Rheumatoid\rbrace{},Humans,Methotrexate,Monocytes,Transcriptome,Tumor Necrosis Factor Inhibitors},
  langid = {english},
  number = {3},
  pages = {e0282564},
  title = {Transcriptomic Profile Comparison of Monocytes from Rheumatoid Arthritis Patients in Treatment with Methotrexate, Anti-TNFa, Abatacept or Tocilizumab},
  volume = {18},
  year = {2023},
}

