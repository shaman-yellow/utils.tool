@article{2009e,
  title = {Protective Effects of {{Eucommia}} Ulmoides {{Oliv}}. Bark and Leaf on Amyloid {$\beta$}-Induced Cytotoxicity.},
  author = {Zhou, Yongqiang and Liang, Min and Li, Weize and Li, Kai and Li, Ping and Hu, Yuzhu and Yang, Zhonglin},
  year = {2009},
  month = nov,
  journal = {Environmental toxicology and pharmacology},
  volume = {28},
  number = {3},
  pages = {342--349},
  address = {{Netherlands}},
  issn = {1872-7077 1382-6689},
  doi = {10.1016/j.etap.2009.05.012},
  abstract = {The objectives of this study were to investigate the protective effects of Eucommia ulmoides Oliv. (EUO) bark and leaf against cytotoxicity induced by  amyloid-{$\beta$} peptide (A{$\beta$}) and to explore their active components. The PC-12 cells  injury mediated by A{$\beta$}(25-35) was employed to assess the neuroprotective effects  of EUO bark, EUO leaf and various compounds. Intracellular Ca(2+) determination,  MTT reduction assay, lactate dehydrogenase leakage evaluation and HO33258/PI  staining were used to quantitatively or qualitatively evaluate cell viability and  injury. The organic solvents partition and the macroporous resin separation were  also applied to tracing the active constituents of EUO bark. Moreover, the  effects of 8 compounds (3 iridoid glucoside, 3 phenylpripanoids and 2 flavonoids)  were tested to identify the active compounds of EUO leaf. The results  demonstrated that the water extracts of EUO barks and leaves, geniposidic acid  and chlorogenic acid could efficiently protect PC-12 cells against the  cytotoxicity of A{$\beta$}(25-35). This research suggests that EUO may represent a  potential treatment strategy for Alzheimer's disease. Meanwhile, geniposidic acid  and chlorogenic acid are the major active constituents of EUO barks and leaves,  respectively.},
  copyright = {Copyright \textcopyright{} 2009 Elsevier B.V. All rights reserved.},
  langid = {english},
  pmid = {21784025},
  file = {/mnt/data/wizard/Zotero/storage/Y38DAWYE/Zhou et al. - 2009 - Protective effects of Eucommia ulmoides Oliv. bark.pdf}
}

