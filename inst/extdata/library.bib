  pmid = {8265504},
  keywords = {*Chickens,Animals,Cholesterol/*analysis/blood,Drugs; Chinese Herbal/*administration \& dosage/pharmacology,Egg Yolk/chemistry,Eggs/*standards,Female,Food; Fortified,Lipids/analysis,Oviposition/*drug effects},
