  journal = {Nucleic Acids Research},
  language = {en},
  month = {1},
  pages = {D1197--D1206},
  publisher = {Oxford University Press (OUP)},
  title = {HERB: a high-throughput experiment- and reference-guided database of traditional Chinese medicine},
  url = {http://dx.doi.org/10.1093/nar/gkaa1063},
  volume = {49},
