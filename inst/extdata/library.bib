  keywords = {Adolescent,Adult,Aged,Aged; 80 and over,CITE-seq,COVID-19,Female,Genomics,Humans,immune response,infection,Male,metabolomics,Middle Aged,multi-omics,proteomics,RNA-Seq,SARS-CoV-2,Severity of Illness Index,Single-Cell Analysis,single-cell RNA-seq,single-cell secretome,single-cell TCR-seq},
