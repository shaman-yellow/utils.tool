  shorttitle = {{{TPM}}, {{FPKM}}, or {{Normalized Counts}}?},
  author = {Zhao, Yingdong and Li, Ming-Chung and Konat{\'e}, Mariam M. and Chen, Li and Das, Biswajit and Karlovich, Chris and Williams, P. Mickey and Evrard, Yvonne A. and Doroshow, James H. and McShane, Lisa M.},
