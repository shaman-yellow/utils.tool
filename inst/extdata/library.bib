  author = {Hussain, Tarique and Yuan, Diaxiu and Tan, Bie and Murtaza, Ghulam and Rahu, Najma and Kalhoro, Muhammed Saleem and Kalhoro, Dildar Hussain and Yin, Yulong},
  year = {2020},
  month = feb,
  journal = {Toxicology in vitro : an international journal published in association with BIBRA},
  volume = {62},
  pages = {104674},
  address = {{England}},
  issn = {1879-3177 0887-2333},
  doi = {10.1016/j.tiv.2019.104674},
  abstract = {This study was conducted to explore the regulatory mechanism of Eucommia ulmoides flavones (EUF) using enterocyte damage model induced by lipopolysaccharide (LPS).  Intestinal porcine epithelial cell line (IPEC-J2) cells were cultured in  Dulbecco's modified eagle medium with high glucose (DMEM-H) medium containing 0  or 10\,{$\mu$}g/mL EUF, 0 or 40\,ng/mL LPS. The results showed that LPS impaired DNA  synthesis, cell viability, mitochondrial function, arrested cell cycle and  induced apoptosis, reduced SOD activity while the EUF treated cells provided  beneficial effect on all these parameters (P\,{$<$}\,0.05). The addition of EUF  increased phosphorylated Akt, I{$\kappa$}B{$\alpha$} and phosphorylated IKK{$\alpha$}/{$\beta$}, but decreased Bax  and Caspase-3 protein expressions in LPS-treated cells (P\,{$<$}\,0.05). For the second  experiment, cells were treated by DMEM-H medium containing 10\,{$\mu$}g/mL EUF+ 40\,ng/mL  LPS or 10\,{$\mu$}g/mL EUF+ 40\,ng/mL LPS+ 10\,{$\mu$}mol/L LY29400. EUF\,+\,LPS\,+\,LY29400  treatment significantly reduced cell viability, proliferation, mitochondrial  bioenergetics parameters, SOD activity, and decreased protein expressions of  PI3K, p-Akt, p-IKK{$\alpha$}/{$\beta$}, p-NF{$\kappa$}B and Bax (P\,{$<$}\,0.05). These findings revealed the  cytoprotective effects of EUF in enterocyte, which may involve the PI3K-NF{$\kappa$}B  signaling pathway, and it provides a theoretical basis for exploration of EUF as  a potential anti-inflammatory compound to intervene intestinal inflammatory  diseases.},
  copyright = {Copyright \textcopyright{} 2019 Elsevier Ltd. All rights reserved.},
  langid = {english},
  pmid = {31669396},
  keywords = {*Eucommiaceae,Animals,Anti-Inflammatory Agents/*pharmacology,Apoptosis/drug effects,Cell Cycle/drug effects,Cell Line,Cell Survival/drug effects,Enterocytes and LPS,Enterocytes/*drug effects/metabolism,Eucommia ulmoides flavones,Flavones/*pharmacology,Inflammation,Lipopolysaccharides/*toxicity,Mitochondria/drug effects/metabolism,NF-kappa B/metabolism,PI3K-NFκB signaling pathway,Superoxide Dismutase/metabolism,Swine},
