@article{2020bp,
  title = {Anti-{{Osteoarthritic Effects}} of a {{Mixture}} of {{Dried Pomegranate Concentrate Powder}}, {{Eucommiae Cortex}}, and {{Achyranthis Radix}} 5:4:1 (g/g) in a {{Surgically Induced}}  {{Osteoarthritic Rabbit Model}}.},
