  issn = {1759-5037},
  journal = {Nature Reviews. Endocrinology},
  keywords = {Animals,Cholesterol,Gene Expression Regulation,Homeostasis,Humans,Lipid Metabolism,Liver X Receptors,Needs Assessment,Signal Transduction,\lbrace{}Translational Research, Biomedical\rbrace{}},
  langid = {english},
  month = {August},
  number = {8},
  pages = {452--463},
  title = {Liver X Receptors in Lipid Signalling and Membrane Homeostasis},
  volume = {14},
