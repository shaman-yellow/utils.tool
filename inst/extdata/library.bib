  keywords = {*environmental adaptability,*eucommia rubber,*Eucommia ulmoides genome,Genome; Plant/*genetics,Hemiterpenes/*metabolism,Hevea/*genetics/*metabolism,Phylogeny,Plant Proteins/genetics},
