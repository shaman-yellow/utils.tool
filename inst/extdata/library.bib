  keywords = {Alkaloids/*chemistry/pharmacology,alpha-Glucosidases/*chemistry/metabolism,Antioxidants/chemistry/*pharmacology,asperflaloids A and B,Aspergillus flavipes DZ-3,Aspergillus/*chemistry,bioactivity,Eucommiaceae/*microbiology,structural identification},
  file = {/mnt/data/wizard/Zotero/storage/PWWHIDNP/Liu et al. - 2021 - Asperflaloids A and B from Aspergillus flavipes DZ.pdf}
