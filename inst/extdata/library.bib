  author = {Ho, Jin-Nyoung and Lee, Yoo-Hyun and Lee, Yun-Dong and Jun, Woo-Jin and Kim, Hye-Kyung and Hong, Bum-Shik and Shin, Dong-Hoon and Cho, Hong-Yon},
