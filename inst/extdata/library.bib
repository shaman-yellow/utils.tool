  pmid = {21569830},
  keywords = {*Phytotherapy,Animals,Antioxidants/metabolism/pharmacology/*therapeutic use,Cell Death/drug effects,Drugs; Chinese Herbal/pharmacology/*therapeutic use,Glutathione/metabolism,L-Lactate Dehydrogenase/metabolism,Male,Motor Activity/drug effects,Muscle; Skeletal/drug effects/pathology,Nerve Fibers/drug effects,Nerve Regeneration/*drug effects/physiology,Neuroprotective Agents/pharmacology/therapeutic use,Oxidative Stress/drug effects,Peripheral Nervous System Diseases/*drug therapy/pathology/physiopathology,Plants; Medicinal,Rats,Rats; Sprague-Dawley,Reactive Oxygen Species/metabolism,Sciatic Nerve/*drug effects/pathology/physiopathology,Trauma; Nervous System/*drug therapy/pathology/physiopathology},
  file = {/mnt/data/wizard/Zotero/storage/3AFDPYR3/Kim et al. - 2011 - Protective effect of GCSB-5, an herbal preparation.pdf}
}

