  journal = {Nature Genetics},
  langid = {english},
  month = {April},
  number = {4},
  pages = {679--692},
  title = {Acetylation of Histone H2B Marks Active Enhancers and Predicts CBP/P300 Target Genes},
  urldate = {2023-04-26},
  volume = {55},
  year = {2023},
}

