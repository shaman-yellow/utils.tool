@article{2018ba,
  title = {{{ProteinExplorer}}: {{A Repository-Scale Resource}} for {{Exploration}} of {{Protein Detection}} in {{Public Mass Spectrometry Data Sets}}},
  shorttitle = {{{ProteinExplorer}}},
  author = {Pullman, Benjamin S. and Wertz, Julie and Carver, Jeremy and Bandeira, Nuno},
