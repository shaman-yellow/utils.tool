  author = {Wang, Jin and Li, Ying and Huang, Wei-Hua and Zeng, Xiang-Chang and Li, Xiao-Hui and Li, Jian and Zhou, Jun and Xiao, Jian and Xiao, Bo and Ouyang, Dong-Sheng and Hu, Kai},
