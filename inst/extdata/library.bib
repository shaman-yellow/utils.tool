}

@article{1999a,
  title = {Improvement in the Turnover Rate of the Stratum Corneum in False Aged Model Rats by the Administration of Geniposidic Acid in {{Eucommia}} Ulmoides {{Oliver Leaf}}.},
  author = {Li, Y. and Metori, K. and Koike, K. and Che, Q. M. and Takahashi, S.},
