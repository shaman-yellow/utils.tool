  pmid = {26782433},
  keywords = {Breeding,Chromosome Mapping,Chromosomes; Plant/genetics,Crosses; Genetic,Eucommiaceae/*genetics/growth \& development,Genetic Linkage,Genotype,Microsatellite Repeats/*genetics,Plant Leaves/*genetics/growth \& development,Quantitative Trait Loci/*genetics},
