  author = {Qu, Jing-Tian and Zhang, Dong-Xuan and Liu, Fang and Mao, Hao-Ping and Ma, Ya-Ke and Yang, Yue and Li, Chun-Xiao and Qiu, Li-Zhen and Geng, Xiao and Zhang, Jian-Mei and Gao, Xiu-Mei and Chen, Lu and Wang, Hong},
  year = {2015},
  journal = {Biological \& pharmaceutical bulletin},
  volume = {38},
  number = {12},
  pages = {1873--1878},
  address = {{Japan}},
  issn = {1347-5215 0918-6158},
  doi = {10.1248/bpb.b15-00444},
  abstract = {Wogonin, a natural flavonoid, is one of the bioactive compounds of the medicinal herb Eucommia ulmoides OLIV. widely used in southeastern Asia for treating  hypertension. However, the molecular mechanisms for the therapeutic benefits  remain largely unclear. The present study investigated the vasodilatory effect of  wogonin and its possible mechanisms. The flavonoid (0.1-100\,\textmu M) caused  concentration-dependent relaxations in endothelium-intact aortic rings  precontracted with norepinephrine (NE, 1\,\textmu M) or potassium chloride (KCl, 60\,mM).  Preincubation with wogonin (10, 100\,\textmu M) for 20\,min significantly inhibited the  contractile responses to NE (0.1, 1, 10\,\textmu M) or KCl (7.5, 15, 30, 60\,mM). Relaxant  responses to wogonin were not inhibited by N(G)-nitro-L-arginine methylester  (100\,\textmu M) or endothelial denudation. In a Ca(2+)-free Krebs' solution, wogonin not  only blocked Ca(2+) influx-dependent vasoconstriction by either NE (1\,\textmu M) or KCl  (100\,mM), but also inhibited NE (1\,\textmu M)-induced tonic contraction, which is  dependent on intracellular Ca(2+) release. Wogonin also suppressed the elevation  of [Ca(2+)]i induced by KCl (60\,mM) after exhausting the calcium store in  sarcoplasmic and endoplasmic reticula with thapsigargin (1\,\textmu M) or by ATP (100\,\textmu M)  in primary vascular smooth muscle cells. These findings suggest that  wogonin-induced responses are mainly due to the inhibition of both intracellular  Ca(2+) release and extracellular Ca(2+) influx.},
  langid = {english},
  pmid = {26632179},
  keywords = {Animals,Aorta,Calcium/metabolism,Drugs; Chinese Herbal/*pharmacology,Endothelium; Vascular/metabolism,Eucommiaceae/*chemistry,Flavanones/*pharmacology,Hypertension/drug therapy/physiopathology,Male,Muscle Contraction/*drug effects,Muscle; Smooth; Vascular/*drug effects/metabolism,NG-Nitroarginine Methyl Ester/pharmacology,Norepinephrine,Phytotherapy,Potassium Chloride,Rats; Wistar,Vasoconstriction/*drug effects,Vasodilator Agents/*pharmacology},
  file = {/mnt/data/wizard/Zotero/storage/YD4CPT2S/Qu et al. - 2015 - Vasodilatory Effect of Wogonin on the Rat Aorta an.pdf}
}

@article{2015k,
