  keywords = {Chromatography; High Pressure Liquid,Drugs; Chinese Herbal/*analysis,Eucommiae Cortex,gray relational analysis,Molecular Conformation,processing methods,simultaneous determination,Tandem Mass Spectrometry,traditional Chinese medicines},
