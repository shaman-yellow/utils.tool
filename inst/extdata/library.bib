  keywords = {*Eucommiaceae,Animals,Anti-Inflammatory Agents/pharmacology/*therapeutic use,Chondrocytes/metabolism,Disease Models; Animal,Eucommia ulmoides Oliv. bark,IL-1β,IL-6,Interleukin-1beta/blood,Interleukin-6/blood,Knee Joint/drug effects/pathology,Matrix Metalloproteinase 3/metabolism,MMP-3,Osteoarthritis,Osteoarthritis/blood/*drug therapy/metabolism/pathology,Phytotherapy,PI3K/Akt signaling pathway,Plant Bark,Plant Extracts/pharmacology/*therapeutic use,Proto-Oncogene Proteins c-akt/metabolism,Rats; Sprague-Dawley},
  file = {/mnt/data/wizard/Zotero/storage/YQ4XS2PL/Xie et al. - 2015 - Eucommia ulmoides Oliv. bark aqueous extract inhib.pdf}
}

