@article{2018m,
  title = {Enrichment and {{Purification}} of {{Aucubin}} from {{Eucommia}} Ulmoides {{Ionic Liquid Extract Using Macroporous Resins}}.},
  author = {Yang, Xinyu and Wei, Mengxia and Tian, Hao and Liu, Tingting and Yang, Lei},
