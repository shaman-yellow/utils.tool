  author = {Fujiwara, Ayaka and Nishi, Mayuko and Yoshida, Shigeo and Hasegawa, Morifumi and Yasuma, Chieko and Ryo, Akihide and Suzuki, Yoshihito},
  year = {2016},
  month = feb,
  journal = {Phytochemistry},
  volume = {122},
  pages = {139--145},
  address = {{England}},
  issn = {1873-3700 0031-9422},
  doi = {10.1016/j.phytochem.2015.11.017},
  abstract = {Cancer stem cells (CSCs) constitute a small population of undifferentiated cells within a tumor that have the ability to self-renew and drive tumor formation,  thus behaving as cancer-initiating cancer cells. Therapeutic interventions that  eliminate CSCs are necessary to completely cure patients, since CSCs are a  crucial source of tumor recurrence and metastasis. An induced CSC-like (iCSCL)  model was recently established using induced pluripotent stem cells (iPSCs). In  this study, a natural product-eucommicin A-was identified from Eucommia ulmoides  leaves by screening for anti-CSC activity using the iCSCL model. Its structure  was elucidated by spectroscopic methods as a quinic acid diester of  3,4,3',4'-tetrahydroxy-{$\beta$}-truxinic acid. Eucommicin A exhibited selective anti-CSC  activity and inhibited tumor sphere formation by iCSCL cells. The results of this  study suggest that eucommicin A could serve as a lead compound in the development  of drugs to abrogate the stemness and self-renewal ability of CSCs.},
  copyright = {Copyright \textcopyright{} 2015 Elsevier Ltd. All rights reserved.},
  langid = {english},
  pmid = {26678322},
  keywords = {Antineoplastic Agents/chemistry/*isolation \& purification/*pharmacology,Cancer stem cell,Drug Resistance; Neoplasm,Eucommia ulmoides,Eucommiaceae,Eucommiaceae/*chemistry,Humans,Lignan,Lignans/chemistry/*isolation \& purification/*pharmacology,Molecular Structure,Neoplastic Stem Cells/*drug effects,β-Truxinate},
  file = {/mnt/data/wizard/Zotero/storage/FDTGKYI8/Fujiwara et al. - 2016 - Eucommicin A, a β-truxinate lignan from Eucommia u.pdf}
}

