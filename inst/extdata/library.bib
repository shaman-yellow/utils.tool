@article{2019ao,
  title = {Diverse {{Pharmacological Activities}} and {{Potential Medicinal Benefits}} of {{Geniposide}}.},
  author = {Zhou, Yan-Xi and Zhang, Ruo-Qi and Rahman, Khalid and Cao, Zhi-Xing and Zhang, Hong and Peng, Cheng},
