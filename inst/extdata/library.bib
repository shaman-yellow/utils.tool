@article{2021di,
  title = {The Structural Basis for the Phospholipid Remodeling by Lysophosphatidylcholine Acyltransferase 3},
  author = {Zhang, Qing and Yao, Deqiang and Rao, Bing and Jian, Liyan and Chen, Yang and Hu, Kexin and Xia, Ying and Li, Shaobai and Shen, Yafeng and Qin, An and Zhao, Jie and Zhou, Lu and Lei, Ming and Jiang, Xian-Cheng and Cao, Yu},
  year = {2021},
