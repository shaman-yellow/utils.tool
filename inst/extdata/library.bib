  keywords = {Algorithms,Animals,\lbrace{}Chromatography, Liquid\rbrace{},\lbrace{}Databases, Factual\rbrace{},Drosophila,Gene Expression Regulation,Gluconeogenesis,Metabolic Networks and Pathways,Metabolome,Metabolomics,\lbrace{}Models, Biological\rbrace{},Tandem Mass Spectrometry,Transcriptome},
