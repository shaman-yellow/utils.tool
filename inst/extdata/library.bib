  shorttitle = {The \lbrace{}\lbrace{}Current Application\rbrace{}\rbrace{} of \lbrace{}\lbrace{}LC-MS\rbrace{}\rbrace{}/\lbrace{}\lbrace{}MS\rbrace{}\rbrace{} in \lbrace{}\lbrace{}Pharmacokinetics\rbrace{}\rbrace{} of \lbrace{}\lbrace{}Traditional Chinese Medicines\rbrace{}\rbrace{} (\lbrace{}\lbrace{}Recent Three Years\rbrace{}\rbrace{})},
  title = {The Current Application of LC-MS/MS in Pharmacokinetics of Traditional Chinese Medicines (Recent Three Years): A Systematic Review},
