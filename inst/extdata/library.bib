@article{2016q,
  title = {Simultaneous {{Determination}} of {{Catalpol}}, {{Aucubin}}, and {{Geniposidic Acid}} in {{Different Developmental Stages}} of {{Rehmannia}} Glutinosa {{Leaves}} by {{High Performance}}  {{Liquid Chromatography}}.},
