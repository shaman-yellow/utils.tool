@article{2021cc,
  title = {Chlorogenic Acid Inhibits Proliferation in Human Hepatoma Cells by Suppressing Noncanonical {{NF-$\kappa$B}} Signaling Pathway and Triggering Mitochondrial Apoptosis.},
  author = {Jiang, Yanfei and Nan, Hao and Shi, Na and Hao, Wenfang and Dong, Juane and Chen, Hongying},
  year = {2021},
  month = mar,
  journal = {Molecular biology reports},
  volume = {48},
  number = {3},
  pages = {2351--2364},
  address = {{Netherlands}},
  issn = {1573-4978 0301-4851},
  doi = {10.1007/s11033-021-06267-3},
  abstract = {Chlorogenic acid (CGA), a phenylpropanoid derived from Eucommia ulmoides Oliver, has been shown to exhibit potent cytotoxic and anti-proliferative activities  against several human cancers. However, the effects of CGA on hepatocellular  carcinoma (HCC) and the underlying mechanisms have not been intensively studied.  In this study, the CGA treatment effects on the viability of human hepatoma cells  were investigated by MTT assay. Our data showed that CGA could dose-dependently  inhibit the activity of human hepatoma cells Hep-G2 and Huh-7, but did not affect  the activity and growth of normal human hepatocyte QSG-7701. The genes and  pathways influenced by CGA treatment were explored by RNA sequencing and  bioinformatics analysis, which identified 323 differentially expressed genes  (DEGs) involved in multiple pharmacological signaling pathways such as MAPK,  NF-{$\kappa$}B, apoptosis and TGF-{$\beta$} signaling pathways. Further analyses by real-time  quantitative PCR, Western blot and flow cytometry revealed that CGA effectually  suppressed the noncanonical NF-{$\kappa$}B signaling pathway, meanwhile it activated the  mitochondrial apoptosis of HCC by upregulation of the BH3-only protein Bcl-2  binding component 3 (BBC3). Our findings demonstrated the potential of CGA in  suppressing human hepatoma cells and provided a new insight into the anti-cancer  mechanism of CGA.},
  langid = {english},
  pmid = {33738723},
  keywords = {*Apoptosis/drug effects,*Signal Transduction,Apoptosis,Carcinoma; Hepatocellular/genetics/*pathology,Cell Line; Tumor,Cell Proliferation/drug effects,Cell Survival/drug effects,Chlorogenic acid,Chlorogenic Acid/chemistry/*pharmacology,Gene Ontology,Hepatocellular carcinoma,Humans,Liver Neoplasms/genetics/*pathology,Mitochondria; Liver/drug effects/*metabolism,NF-kappa B/*metabolism,Transcriptome,Transcriptome/drug effects/genetics,Viability},
  file = {/mnt/data/wizard/Zotero/storage/URFUL424/Jiang et al. - 2021 - Chlorogenic acid inhibits proliferation in human h.pdf}
}

