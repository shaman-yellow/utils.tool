  pmid = {25366761},
  keywords = {*DNA Copy Number Variations,*Microsatellite Repeats,*Polymorphism; Restriction Fragment Length,Eucommiaceae/*genetics},
  file = {/mnt/data/wizard/Zotero/storage/IXR7LYDN/Li et al. - 2014 - Genetic diversity and relationships among Chinese .pdf}
}

