@article{2005a,
  title = {{[Isolation, purification and identification of geniposidic acid in Eucommia ulmoides oliv. by silica column chromatography, reversed-phase high performance  liquid chromatography, liquid chromatography-electrospray ionization mass  spectrometry and nuclear magnetic resonance].}},
  author = {Cao, Hui and Chen, Xiaoqing and Xlao, Jianbo and Tang, Zhaoqi and Ouyang, Dongsheng},
  year = {2005},
