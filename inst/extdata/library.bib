@article{2021ce,
  title = {Preparation of {{Eucommia}} Ulmoides Lignin-Based High-Performance Biochar Containing Sulfonic Group: {{Synergistic}} Pyrolysis Mechanism and Tetracycline Hydrochloride  Adsorption.},
  author = {Chen, Yicong and Liu, Jianting and Zeng, Qibin and Liang, Zuxue and Ye, Xiaoxia and Lv, Yuancai and Liu, Minghua},
  year = {2021},
